<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
            android:layout_width="62dp"
            android:layout_height="55dp"
            app:layout_constraintCircle="@id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="60"
            android:id="@+id/iv1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/iv2"/>

    <ImageView
            android:layout_width="61dp"
            android:layout_height="61dp"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="120"
            app:layout_constraintCircle="@id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/iv2"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv3"/>

    <ImageView
            android:layout_width="64dp"
            android:layout_height="57dp"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="180"
            app:layout_constraintCircle="@id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/iv3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/iv4"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="58dp"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="240"
            app:layout_constraintCircle="@id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/iv4"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView"/>

    <ImageView
            android:layout_width="70dp"
            android:layout_height="63dp"
            app:layout_constraintCircle="@id/textView"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="300"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/iv5"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/iv6"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"/>

    <ImageView
            android:layout_width="60dp"
            android:layout_height="58dp"
            app:layout_constraintCircle="@id/textView"
            app:layout_constraintCircleRadius="@dimen/imageContainer_circleRadius"
            app:layout_constraintCircleAngle="360"
            app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/iv6"
            app:layout_constraintTop_toTopOf="@+id/iv1"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>

</merge>